<script type="text/javascript">
  function unfollowartist(artist_id){
    if (confirm('Are you sure?')) {
          $.ajax({
                  type: "GET",
                  url: "<%= unfollow_artist_path %>" ,
                  data:{artist_id: artist_id},
                  success: function(data){
                    if(data.res == 1){
                      $('li#following_li_'+artist_id).hide();
                    }
                  }
          });

    }

  }
</script>
