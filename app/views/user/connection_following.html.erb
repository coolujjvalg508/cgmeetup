                <li><a href="#">Last Name</a></li>
                <li><a href="#">Recent </a></li>
              </ul>
            </li>
          </ul>
        </div>
        <div class="clearfix">
          
          <div class="innerleft clearfix col-sm-12 col-xs-12">
            <ul> <!-- dir-paginate="x in following_data|orderBy:sortKey:reverse|filter:search|itemsPerPage:10" -->
               <% if @following.present? %>
                    
                    <% @following.each_with_index do |artist,index|
                      #  abort(artist.artist.country.to_json)
                     %>
                      
                    <li class="clearfix" id='following_li_<%= artist.artist.id %>'>

                      <div class="connect-following-profile pull-left">
                      <% if artist.artist.present? %>
                          <%= image_tag(artist.artist.image.thumb.url, :alt => "user",:class=>"img-responsive") %>
                      <% end %>  
                      </div>
                      <div class="connect-profile clearfix">
                        <div class="user-title"><%= artist.artist.present? ? artist.artist.firstname : '' %> <%= artist.artist.present? ? artist.artist.lastname : ''  %></div>
                        <div class="user-position-txt"><i class="fa fa-briefcase marginR5" aria-hidden="true"></i> <%= artist.artist.present? ? artist.artist.professional_headline : '' %></div>
                        <div class="user-position-txt"><i class="fa fa-map-marker marginR5" aria-hidden="true"></i> <%= artist.artist.present? ? artist.artist.city+', ' : '' %> <%= artist.artist.country.present? ? artist.artist.country.name : '' %></div>
                   
                        <button onClick='unfollowartist(<%= artist.artist.id %>)' type="button" class="red-btn border-radius marginR5"><i class="fa fa-check-circle" aria-hidden="true"></i> Unfollow</button>

                        <span class="follow-companies">
                        <%= 
                          artist_id = 0
                          if artist.artist.present?
                             artist_id  = artist.artist.id
                          end  
                          artist_like = get_artist_like(artist_id)  
                          artist_like[:user_follow_me]
                         %></span> <span class="white-txt txt13 marginR5"> <i class="fa fa-thumbs-up marginR5" aria-hidden="true"></i> <%= 
                          artist_like = get_artist_like(artist_id)  
                          artist_like[:user_like_me]
                         %></span> <span class="white-txt txt13 marginR5"><i class="fa fa-eye marginR5" aria-hidden="true"></i> 321757</span> 
                         </div>
                      <div class="connection-followers-indent clearfix pull-right">
