                            <%= image_tag("database/followers-4.jpg", :alt => "Followers",:class=>"img-responsive") %>
                          </center>
                        </div>
                      </div>
                    </li>
                   <% end %>   
              
              <% else %>
                <li class="text-center err-msg no-result">No Result Found!</li>

              <% end %> 
