<!-- Download Content Start -->
<section class="clearfix browseallcompanies">



<!-- User Followers Start -->
<div class="container-fluid download-indent clearfix">
<div class="container">

<div class="faq-acco">

<div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
    <% @faq_data.each do |faq_data| %>
        <div class="panel panel-default">
            <div class="panel-heading" role="tab" id="headingOne">
                <h4 class="panel-title">
                    <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapse<%= faq_data.id %>" aria-expanded="true" aria-controls="collapse<%= faq_data.id %>">
                        <i class="more-less glyphicon glyphicon-plus"></i>
                        <%= faq_data.question %>
                    </a>
                </h4>
            </div>
            <div id="collapse<%= faq_data.id %>" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne">
                <div class="panel-body">
                      <%= faq_data.answer.html_safe %>
                </div>
            </div>
        </div>
    <% end %>
    </div>


</div>


</div>
</div>
</section>
<!-- Footer Start -->
<footer class="footer clearfix padding">
 <%= render 'layouts/lastfooter' %>
</footer>